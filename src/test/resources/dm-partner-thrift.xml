        <property name="timeout" value="5000"/>
        <property name="remoteAppkey" value="com.sankuai.hotel.dm.partner"/>
        <property name="remoteServerPort" value="9202"/>
        <property name="appKey" value="com.sankuai.hotel.dm.autotest"/>
