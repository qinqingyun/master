<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="TestMeishiHuiCreateOrderTest" thread-count="4">
        <!--设置双写标识-->
        <test name="以老为主">
            <parameter name="DoubleWriteMode" value="OLD"/>
            <classes>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestOriginalScenes_NEW"/>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestDiscountScenes_New"/>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestMTShopPromo_New"/>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestMerchantPromo_New"/>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestZeroOrder_New"/>


            </classes>
        </test>
        <test name="以新为主">
            <parameter name="DoubleWriteMode" value="NEW"/>
            <classes>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestOriginalScenes_NEW"/>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestDiscountScenes_New"/>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestMTShopPromo_New"/>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestMerchantPromo_New"/>
                <class name="com.meituan.qa.meishi.Hui.cases.huifullproce.TestZeroOrder_New"/>

            </classes>
        </test>

    <parameter name="maxRetries" value="3"/>
    <parameter name="sleepBetweenRetries" value="1000"/>

</suite>