<suite name="testSuite" parallel="true" thread-count="8">
    <parameter name="team_id" value="179"/>
    <!--重构后case-->
    <test name="以老为主,重构后">
        <parameter name="DoubleWriteMode" value="OLD_MAIN"/>
        <classes>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.MeiTuanAppTest"/>
            <methods>
                <exclude name="mtShopPromoZeroTest"/>
                <exclude name="mtCouponPromoZeroTest"/>
                <exclude name="mtDiscountAndCouponPromoZeroTest"/>
            </methods>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.DianPingAppTest"/>
        </classes>
    </test>
    <test name="以新为主,重构后">
        <parameter name="DoubleWriteMode" value="NEW_MAIN"/>
        <classes>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.MeiTuanAppTest"/>
            <methods>
                <include name="mtResvZeroTest"/>
            </methods>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.DianPingAppTest"/>
        </classes>
    </test>
    <parameter name="maxRetries" value="4"/>
</suite>