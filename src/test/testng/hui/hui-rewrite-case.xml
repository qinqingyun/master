<suite name="testSuite" parallel="true" thread-count="8">
    <!--设置双写标识-->
    <test name="单写新">
        <parameter name="DoubleWriteMode" value="NEW_ONLY"/>
        <classes>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.MeiTuanAppTest"/>
            <methods>
                <include name="mtOriginTest"/>
                <include name="mtDiscountTest"/>
                <include name="mtShopPromoTest"/>
                <include name="mtCouponPromoTest"/>
            </methods>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.DianPingAppTest"/>
            <methods>
                <include name="dpOriginTest"/>
                <include name="dpDiscountTest"/>
                <include name="dpShopPromoTest"/>
                <include name="dpCouponPromoTest"/>
            </methods>
<!--            <class name="com.meituan.qa.meishi.Hui.cases.scene.DianPingMTest"/>-->
        </classes>
    </test>
    <test name="以新为主">
        <parameter name="DoubleWriteMode" value="NEW_MAIN"/>
        <classes>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.MeiTuanAppTest"/>
            <methods>
                <include name="mtOriginTest"/>
                <include name="mtDiscountTest"/>
                <include name="mtShopPromoTest"/>
                <include name="mtCouponPromoTest"/>
            </methods>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.DianPingAppTest"/>
            <methods>
                <include name="dpOriginTest"/>
                <include name="dpDiscountTest"/>
                <include name="dpShopPromoTest"/>
                <include name="dpCouponPromoTest"/>
            </methods>
<!--            <class name="com.meituan.qa.meishi.Hui.cases.scene.DianPingMTest"/>-->
        </classes>
    </test>
    <test name="以老为主">
        <parameter name="DoubleWriteMode" value="OLD_MAIN"/>
        <classes>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.MeiTuanAppTest"/>
            <methods>
                <include name="mtOriginTest"/>
                <include name="mtDiscountTest"/>
                <include name="mtShopPromoTest"/>
                <include name="mtCouponPromoTest"/>
            </methods>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.DianPingAppTest"/>
            <methods>
                <include name="dpOriginTest"/>
                <include name="dpDiscountTest"/>
                <include name="dpShopPromoTest"/>
                <include name="dpCouponPromoTest"/>
            </methods>
<!--            <class name="com.meituan.qa.meishi.Hui.cases.scene.DianPingMTest"/>-->
        </classes>
    </test>

</suite>