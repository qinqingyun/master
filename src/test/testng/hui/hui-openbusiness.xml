<suite name="testSuite" parallel="true" thread-count="8">
    <!--设置双写标识-->
    <test name="以老为主">
        <parameter name="DoubleWriteMode" value="OLD"/>
        <classes>
            <class name="com.meituan.qa.meishi.Hui.cases.huiopenbusinessweb.TestCreateOrder"/>
        </classes>
    </test>
    <test name="以新为主">
        <parameter name="DoubleWriteMode" value="NEW"/>
        <classes>
            <class name="com.meituan.qa.meishi.Hui.cases.huiopenbusinessweb.TestCreateOrder"/>
        </classes>
    </test>
    <test name="以新为主,重构后">
        <parameter name="DoubleWriteMode" value="OLD_MAIN"/>
        <classes>
            <class name="com.meituan.qa.meishi.Hui.cases.scene.MeiTuanAppTest"/>
            <methods>
                <include name="mtOriginTest"/>
                <include name="mtDiscountTest"/>
                <include name="mtShopPromoTest"/>
                <include name="mtCouponPromoTest"/>
            </methods>
        </classes>
    </test>

</suite>